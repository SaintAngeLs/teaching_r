#include <stdio.h>
#include <string.h>

#define TOTAL_QUESTIONS 50
#define MAX_SCORE 20
#define BUFFER_SIZE 256

int main() {
    FILE *original_answers = fopen("original_answers.txt", "r");
    FILE *student_answers = fopen("students_answers.txt", "r");
    
    if (original_answers == NULL || student_answers == NULL) {
        printf("Error opening files\n");
        return 1;
    }
    
    char original_line[BUFFER_SIZE];
    char student_line[BUFFER_SIZE];
    int correct_answers = 0;

    for (int i = 0; i < TOTAL_QUESTIONS; i++) {
        fgets(original_line, BUFFER_SIZE, original_answers);
        fgets(student_line, BUFFER_SIZE, student_answers);

        char *original_answer = strchr(original_line, ')') + 2;
        char *student_answer = strchr(student_line, ')') + 2;

        if (strcmp(original_answer, student_answer) == 0) {
            correct_answers++;
        }
    }

    float score = ((float)correct_answers / TOTAL_QUESTIONS) * MAX_SCORE;

    printf("The student's score is %.2f out of 20\n", score);

    fclose(original_answers);
    fclose(student_answers);
    
    return 0;
}
