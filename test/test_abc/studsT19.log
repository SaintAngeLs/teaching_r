            correct_answer[strlen(correct_answer) - 1] = '\0'; // remove new line
            if (strstr(student_answer, correct_answer) != NULL) {
                score++;
            }
        }
        total++;
    }

    fclose(student_file);
    fclose(correct_file);

    int mark = (score * 20) / total;
    printf("The student's mark is: %d\n", mark);

    return 0;
}
