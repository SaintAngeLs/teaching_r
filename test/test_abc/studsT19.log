#include <stdio.h>

int main() {
    // Open the files
    FILE *studentFile, *correctFile;
    studentFile = fopen("file_student_answers.txt", "r");
    correctFile = fopen("file_with_correct_answers.txt", "r");

    if (studentFile == NULL || correctFile == NULL) {
        printf("Error opening files.\n");
        return 1;
    }

    // Read student's answers and compare with correct answers
    char studentAnswer, correctAnswer;
    int score = 0;
    int questionNumber = 1;

    while ((studentAnswer = fgetc(studentFile)) != EOF) {
        correctAnswer = fgetc(correctFile);

        if (studentAnswer == correctAnswer) {
            score++;
        }

        if (correctAnswer == '\n') {
            // Print question number and mark "+" if the answer is correct
            printf("%d. ", questionNumber);
            printf("%c ", studentAnswer);
            if (studentAnswer == correctAnswer) {
                printf("+");
            }
            printf("\n");

            questionNumber++;
        }
    }

    // Close the files
    fclose(studentFile);
    fclose(correctFile);

    // Print the student's score out of 20
    printf("\nStudent's Score: %d/20\n", score);

    return 0;
}


